Sub D2MBorger
  OnError GoTo OOPS
 	If ErTabelAaben("Borgerhøringsliste")
		Then
			If Ask("Der er allerede en Borgerhøringsliste åben! Vil du gemme ændringer, før du går videre?", "Ja","Nej")
				Then
					glExit=TRUE
					Exit Sub
				Else
					Close Table Borgerhøringsliste
			End If
		End If
	 	If ErTabelAaben("Borgerliste")
		Then
					Close Table Borgerliste
		End If


 	If NOT ErTabelAaben(gsCPRtabel)
 		Then
 			'Print "Åbner GeoBorger"
 		Open Table gsCPRFolder+gsCPRtabel
 	End If

		Do Case giMetode
			Case 1
				'Print "Intersects"
			 		Select CPR, Alder, Navn, CO_NAVN, Adresse, Postdistrikt, Personnummer, ADRESSE_ID, ADRESSELINK_ID, UDVadresseID From gsCPRtabel Where obj Intersects oBuffer AND Alder>=18 Order By Adresse, Navn Into Borgerliste Noselect
 			Case 2
				'Print "Within"
		 						 		Select CPR, Alder, Navn, CO_NAVN, Adresse, Postdistrikt, Personnummer, ADRESSE_ID, ADRESSELINK_ID, UDVadresseID From gsCPRtabel Where obj Within oBuffer AND Alder>=18 Order By Adresse, Navn Into Borgerliste Noselect
		End Case


	  Create Table "Borgerhøringsliste" (Rolle Char(20), Ejendomsbeliggenhed Char(75), Matrikeladresse Char(80), ADRESSERINGSNAVN Char(50), CO_navn Char(35), Adresse Char(44), Ejers_udvidede_adresse Char(35), Postdistrikt Char(35),
  Ejendomsnummer Char(75), Matrikelnummer Char(75), D2M Char(12), CPR_CVR_nr Float, aAdresseIDCOL Char(12), UDVadresseID Char(18), Emne Char(120), PNO Char(12))
   file gsD2MFolder+"Borgerhøringsliste.tab" TYPE NATIVE Charset "WindowsLatin1"
	Create Map For Borgerhøringsliste CoordSys Earth Projection 8, 115, "m", 9, 0, 0.9996, 500000, 0 Bounds (400000,6000000) (800000,6500000)

	Run Command "Insert Into Borgerhøringsliste ( ADRESSERINGSNAVN, CO_navn, Adresse, Postdistrikt, D2M, CPR_CVR_nr, "+aAdresseIDCOL+", UDVadresseID)	Select Navn, CO_NAVN, Adresse, Postdistrikt, CPR, Personnummer,"+aAdresseIDCOL+", UDVadresseID From Borgerliste"

	Update Borgerhøringsliste Set Ejendomsbeliggenhed=Adresse, Rolle="Beboer", Emne=gsEmne
	Add Column "Borgerhøringsliste" (Matrikeladresse) From gsEjertabel Set To Matrikeladresse Where Contains
	Commit Table Borgerhøringsliste


	If glJoin And NOT glD2Mejer
	Then
	iJoinTableCols=TableInfo("JoinTable", TAB_INFO_NCOLS)
		ReDim sJoinTableCols(iJoinTableCols)
		ReDim sBorgerListeCols(iJoinTableCols)
	For i = 1 To iJoinTableCols
		sJoinTableCols(i)=ColumnInfo("JoinTable", "COL"+i, COL_INFO_NAME)

		If NOT ColExists("Borgerhøringsliste", sJoinTableCols(i)) And sJoinTableCols(i) NOT in ("Rolle","ADRESSERINGSNAVN","CO_navn","Adresse","CPR_CVR_nr","Emne", "PNO")
		Then Alter Table "Borgerhøringsliste" (Add sJoinTableCols(i) Char(150))
		sBorgerListeCols(i)=sJoinTableCols(i)
		Else sBorgerListeCols(i)=sJoinTableCols(i)+"_TMP"
		Alter Table "Borgerhøringsliste" (Add sBorgerListeCols(i) Char(150))
		End If

		Run Command "Add Column "+eye+"Borgerhøringsliste"+eye+" ("+sBorgerListeCols(i)+") From JoinTable Set To "+sJoinTableCols(i)+" Where Intersects"
		Commit Table Borgerhøringsliste
	Next

	'For i=1 To UBound(sBorgerListeCols)
		'Print sBorgerListeCols(i)+" sBorgerListeCols nr.:"+i
	'Next

	ElseIf glJoin And glD2Mejer
	Then
		ReDim sJoinTableCols(UBound(sEjerListeCols))
		ReDim sBorgerListeCols(UBound(sEjerListeCols))
For i= 1 To UBound(sEjerListeCols)
		sBorgerListeCols(i)=sEjerListeCols(i)
		 Alter Table "Borgerhøringsliste" (Add sBorgerListeCols(i) Char(150))
		Run Command "Add Column "+eye+"Borgerhøringsliste"+eye+" ("+sBorgerListeCols(i)+") From JoinTable Set To "+sJoinTableCols(i)+" Where Intersects"
		Commit Table Borgerhøringsliste
	Next
	End If 'glJoin

	sCOLsBorgerliste="Rolle, Ejendomsbeliggenhed, Matrikeladresse, ADRESSERINGSNAVN, CO_navn, Adresse, Postdistrikt, D2M, CPR_CVR_nr, "+aAdresseIDCOL+", UDVadresseID, Emne, PNO"
 	If glJoin
	Then
		For iJoinTableCols=1 To UBound(sBorgerListeCols())
		sCOLsBorgerliste=sCOLsBorgerliste+", "+sBorgerListeCols(iJoinTableCols)
		'Print "sCOLsBorgerliste: "+sCOLsBorgerliste
		Next
	End If

		If glD2Mejer 'Altså ejere og borgere
			Then
			Select * From Ejerliste, Borgerhøringsliste Where Ejer_CPR_CVR=CPR_CVR_nr Into atemp
			Select * From Borgerhøringsliste Where CPR_CVR_nr In(Select Ejer_CPR_CVR From atemp) Into b
				Delete From b 'Ejere skal ikke også høres som borgere
			Update Borgerhøringsliste Set Emne=gsEmne
			Commit Table Borgerhøringsliste
			Pack Table Borgerhøringsliste Data
 	Run Command "Insert Into D2M ( "+sCOLsBorgerliste+") Select "+sCOLsBorgerliste+" From Borgerhøringsliste"
 	'Print "Insert Into D2M ( "+sCOLsBorgerliste+") Select "+sCOLsBorgerliste+" From Borgerhøringsliste"
			Commit Table D2M
		Else
				If ErTabelAaben("D2M")
				Then
					Close Table D2M
				End If

			Commit Table Borgerhøringsliste As gsD2MFolder+"D2M.TAB"
			Open Table gsD2MFolder+"D2M.TAB"
				'Browse * From D2M

		End If '  glD2Mejer
	Browse * From Borgerhøringsliste

  Exit Sub
  OOPS:
  Print Error$()
  Resume Next
End Sub 'D2MBorger
